#!/usr/bin/env node

'use strict';

const Xweb = require("xweb");
const path = require('path');

const web = new Xweb();

web.config({
  path: path.resolve(__dirname, '../xweb/test'),
  sessionPath: path.resolve(__dirname, '../../')
});

web.listen(3000, () => {
  console.log("server is running at http://localhost:3000");
});